@page
@using FysioDanmark_Project_Website.Interfaces
@model FysioDanmark_Project_Website.CheckOut

<!DOCTYPE html>

<title>Enter Customer Info</title>
<form method="post">
    <div class="form-group">
        <label asp-for="@Model.Clients.Id" class="control-label"> Id </label>
        <input asp-for="@Model.Clients.Id" class="form-control"/>
        <span asp-validation-for="@Model.Clients.Id" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.Clients.Name" class="control-label">Name </label>
        <input asp-for="@Model.Clients.Name" class="form-control" />
        <span asp-validation-for="@Model.Clients.Name" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Place Order" class="btn btn-primary" />
    </div>
</form>